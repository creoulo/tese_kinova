<launch>
  <node name="processing_node" pkg="experiment_1" type="filtering_and_segmentation">
    <rosparam>
      cloud_topic: "/camera/depth/points"
      frame_id: "/camera_link_optical"
      voxel_leaf_size: 0.03 <!-- mm -->
      x_filter_min: -2.5 <!-- m -->
      x_filter_max: 2.5 <!-- m -->
      y_filter_min: -2.5 <!-- m -->
      y_filter_max: 2.5 <!-- m -->
      z_filter_min: -2.5 <!-- m -->
      z_filter_max: 2.5 <!-- m -->
      r_max: 255
      r_min: 100
      g_max: 255
      g_min: 100
      b_max: 255
      b_min: 0
      cluster_tolerance: 0.03 <!-- m -->
      min_cluster_size: 10
      max_cluster_size: 50
    </rosparam>
  </node>
</launch>
